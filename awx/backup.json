    "ask_credential_on_launch": true, 
    "extra_vars": "---\n# This name is used for the Heat stack and as a prefix for the\n# cluster node hostnames.\n#nfs_name: nfs\n\nnfs_deploy_user: 'centos'\n\nnfs_net:\n  - { net: \"caastest-U-internal\", subnet: \"caastest-U-internal\" }\n\nnfs_groups:\n  - \"{{ nfs_group_server }}\"\n#cluster_gw_group: \"login\"\n\nnfs_group_server:\n  name: \"server\"\n  flavor: \"j1.small\"\n  image: \"centos-7-20190104\"\n  user: \"centos\"\n  num_nodes: 1\n  root_volume_size: \"{{ nfs_volume_size }}\"\n  node_resource: \"Cluster::NodeWithVolume\" \n  nodenet_resource: \"Cluster::NodeNet1\"\n  extra_groups:\n    - \"nfs_server\"\n\n# This name is used for the Heat stack and as a prefix for the\n# cluster node hostnames.\n#slurm_name: slurm\n\nslurm_deploy_user: 'centos'\n\nslurm_net:\n  - { net: \"caastest-U-internal\", subnet: \"caastest-U-internal\" }\n\nslurm_groups:\n  - \"{{ slurm_login }}\"\n  - \"{{ slurm_compute }}\"\n\nslurm_gw_group: \"login\"\n\nslurm_login:\n  name: \"login\"\n  flavor: \"j1.small\"\n  image: \"centos-7-20190104\"\n  user: \"centos\"\n  num_nodes: 1\n  root_volume_size: 0\n  node_resource: \"Cluster::Node\" \n  extra_groups:\n    - \"nfs_clients\"\n  nodenet_resource: \"Cluster::NodeNet1WithPreallocatedFIP\"\n  nodenet_fips:\n    - uuid: \"{{ floating_ip_uuid }}\"\n      ip: \"{{ floating_ip }}\"\n\nslurm_compute:\n  name: \"compute\"\n  flavor: \"j2.tiny\"\n  image: \"centos-7-20190104\"\n  user: \"centos\"\n  num_nodes: \"{{ num_compute_nodes }}\"\n  root_volume_size: 0\n  node_resource: \"Cluster::Node\" \n  nodenet_resource: \"Cluster::NodeNet1\"\n  extra_groups:\n    - \"nfs_clients\"\n\n# Define a list of SLURM partitions to create.\nopenhpc_slurm_partitions: \n  - name: caas\n    groups:\n      - \"{{ slurm_compute }}\"\n\n# A list of OpenHPC runtime libraries to install on compute and control nodes\nopenhpc_packages:\n  - strace\n  - flex\n  - bison\n  - cfitsio\n  - cfitsio-devel\n  - cfitsio-docs\n  - wcslib\n  - wcslib-utils\n  - wcslib-devel\n  - wcstools\n  - ncurses\n  - ncurses-devel\n  - readline\n  - readline-devel\n  - boost\n  - boost-devel\n  - boost-python\n  - boost-math\n  - boost-doc\n  # OpenHPC bits\n  - hdf5-gnu-ohpc\n  - phdf5-gnu-mvapich2-ohpc\n  - gnu-compilers-ohpc\n  - mvapich2-gnu-ohpc\n  - openmpi-gnu-ohpc\n  - imb-gnu-mvapich2-ohpc\n  - imb-gnu-openmpi-ohpc\n  - fftw-gnu-mvapich2-ohpc\n  - fftw-gnu-openmpi-ohpc\n  - openblas-gnu-ohpc\n  - scalapack-gnu-mvapich2-ohpc\n  - gsl-gnu-ohpc\n  - python-scipy-gnu-mvapich2-ohpc\n  - python-scipy-gnu-openmpi-ohpc\n  - python34\n  - python34-devel\n  - python-virtualenv\n  - infiniband-diags\n  - emacs\n  - vim\n\n# kdump not required\nkdump_enable: false\n\njasmin_homedir: \"/jasmin\"\njasmin_group_name: \"jasmin\"\njasmin_groups:\n  - name: \"{{ jasmin_group_name }}\"\n    gid: 2000\n\njasmin_users: \n  - username: \"hsilk\"\n    groups: [ \"{{ jasmin_group_name }}\" ]\n    name: \"Holly Silk <holly@stackhpc.com>\"\n    home: \"{{jasmin_homedir}}/hsilk\"\n    uid: 3390\n    ssh_key:\n      - \"ssh-rsa  hsilk@Hollys-MacBook-Air.local\"\n  - username: \"stig\"\n    groups: [ \"{{ jasmin_group_name }}\" ]\n    name: \"Stig Telfer <stig@stackhpc.com>\"\n    home: \"{{jasmin_homedir}}/stig\"\n    uid: 3391\n    ssh_key:\n      - \"ssh-rsa  stig@skellig.local\"", 
