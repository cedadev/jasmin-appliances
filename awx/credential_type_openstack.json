[
  {
    "asset_type": "credential_type",
    "name": "openstack",
    "kind": "cloud",
    "inputs": {
      "fields": [
        {
          "id": "auth_url",
          "type": "string",
          "label": "Auth URL"
        },
        {
          "id": "token",
          "type": "string",
          "label": "Token"
        },
        {
          "id": "project_id",
          "type": "string",
          "label": "Project"
        }
      ]
    },
    "injectors": {
      "env": {
        "OS_TOKEN": "{{ token }}",
        "OS_AUTH_URL": "{{ auth_url }}",
        "OS_AUTH_TYPE": "token",
        "OS_PROJECT_ID": "{{ project_id }}"
      }
    }
  }
]
